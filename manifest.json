{
  "manifest_version": 3,
  "name": "West Website Detector",
  "version": "1.0",
  "description": "Detects malicious websites by scanning URLs.",
  "permissions": ["tabs", "storage", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon.png"
  }
}
